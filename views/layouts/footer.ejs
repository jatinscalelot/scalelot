<div class="modal fade apply_model" id="careerModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true" ng-controller="careerController">
   <div class="modal-dialog modal-xl modal-dialog-centered">
      <div class="modal-content">
         <div class="contact-form apply-now-form-cos ">
           <div class="modal-header p-0 pb-4">
              <h2 class="modal-title text-white" id="exampleModalLabel">Apply For <span id="applyFor" class="position"></span></h2>
              <div class="p-0 border-0 modal-header"><button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button></div>
           </div>
          <!-- <h2>Apply Now</h2> -->
          <div class="quote-form p-0 mt-4 game-landing-form position-relative d-flex flex-wrap">
            <div class="d-flex align-items-center justify-content-between quote-input-main col-12">
              <label class="form-label">Name <span>*</span></label>
              <div class="input-main d-flex align-items-start gap-3">
                <div class="w-100">
                  <input type="text" class="w-100 form-control" placeholder="First Name" name="first_name" ng-model="career.first_name">
                </div>
                <div class="w-100">
                  <input type="text" class="w-100 form-control" placeholder="Middle Name" name="middle_name" ng-model="career.middle_name">
                </div>
                <div class="w-100">
                  <input type="text" class="w-100 form-control" placeholder="Last Name" name="last_name" ng-model="career.last_name">
                </div>
              </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 pe-md-2">
               <div class="d-flex align-items-center quote-input-main">
                 <label class="form-label">Email <span>*</span></label>
                 <div class="input-main ">
                  <input type="text" class="w-100 form-control" placeholder="Enter Your Email ID" name="email" ng-model="career.email">
                 </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 ps-md-2">
               <div class="d-flex align-items-center quote-input-main">
                 <label class="form-label">Contact Number<span>*</span></label>
                 <div class="group-input mob">
                     <input type="phone" id="phone" ng-model="career.phone" placeholder="Phone Number">
                 </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 pe-md-2">
               <div class="d-flex align-items-center quote-input-main">
                 <label class="form-label">City<span>*</span></label>
                 <div class="input-main ">
                  <input type="text" class="w-100 form-control" placeholder="City" name="city" ng-model="career.city">
                 </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 ps-md-2">
               <div class="d-flex align-items-center quote-input-main">
                  <label class="form-label">Experience<span>*</span></label>
                  <div class="input-main ">
                    <select class="w-100 form-control" name="experience" ng-model="career.experience">
                      <option value="">-Select-</option>
                      <option value="Fresher">Fresher</option>
                      <option value="1-3 Year">1-3 Year</option>
                      <option value="3-5 Year">3-5 Year</option>
                      <option value="More than 5 Year">More than 5 Year</option>
                    </select>
                  </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 pe-md-2">
               <div class="d-flex align-items-center quote-input-main">
                  <label class="form-label">Select Position <span>*</span></label>
                  <div class="input-main ">
                    <select class="w-100 form-control" name="position" ng-model="career.position">
                      <option value="">-Select-</option>
                      <option value="Junior">Junior</option>
                      <option value="Senior">Senior</option>
                      <option value="Team Leader">Team Leader</option>
                    </select>
                  </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 ps-md-2">
               <div class="d-flex align-items-center quote-input-main">
                  <label class="form-label">Joining<span>*</span></label>
                  <div class="input-main">
                    <select class="w-100 form-control" name="joining" ng-model="career.joining">
                      <option value="">-Select-</option>
                      <option value="Immediately">Immediately</option>
                      <option value="In a Week">In a Week</option>
                      <option value="In a Month">In a Month</option>
                    </select>
                  </div>
               </div>
            </div>
            <div class="col-12 mt-2 mt-sm-3">
               <div class="d-flex align-items-start quote-input-main">
                 <label class="form-label">Message</label>
                 <div class="input-main d-flex align-items-center gap-3">
                  <textarea class="w-100 form-control" placeholder="Write a Message" rows="2" name="message" ng-model="career.message"></textarea>
                 </div>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 pe-md-2">
               <div class="d-flex align-items-start quote-input-main">
                  <label class="form-label">Upload CV<span>*</span></label>
                  <div class="input-main upload-file-cos">
                   <input class="form-control" type="file" id="choose-file" name="resume" onchange="angular.element(this).scope().fileSelectedforresume(this)"><label class="w-100 form-label" for="choose-file">Choose file</label>
                  </div>
                  <span class="limit-file">File Size Should be Less than 20 MB</span>
               </div>
            </div>
            <div class="col-12 col-md-6 mt-2 mt-sm-3 ps-md-2">
               <div class="d-flex align-items-start quote-input-main">
                 <label class="form-label">Upload Workflow</label>
                 <div class="input-main upload-file-cos">
                  <input class="form-control" type="file" id="choose-Workflow" onchange="angular.element(this).scope().fileSelectedforWorkflow(this)">
                  <label class="w-100 form-label" for="choose-Workflow">Choose file</label>
                 </div>
               </div>
            </div>
            <div class="col-12 mt-2 mb-4">
               <div class="d-flex align-items-center justify-content-center">
                  <button type="button" class="submit-form-cos btn btn--primary" ng-click="onSubmitCareer()">Submit</button>
               </div>
            </div>
          </div>
        </div>
      </div>
   </div>
</div>
<!-- modal end -->

